extends layout/layout

block vars
    -title = ''
    -bodyClass = 'page-ket'

block container
   .bnh.bn-text
      .bnh-wrap 
         .bnh-slider 
            .b-sw 
               +Swiper()
                  .swiper-slide.col
                     .bnh-item
                        .b-img
                           .h-full
                              +img("bn-ket.jpg")
                        .container 
                           .b-content(data-aos="fade-left")
                              .b-head
                                 span.t20.c-text KET
                                 .t-text
                                    | VIẾT, NÓI MẠCH LẠC - TƯ DUY SÂU - CHÍNH XÁC NGỮ PHÁP
                              .b-btn
                                 +Btn("Đăng ký học thử miễn phí")
                        a(href="https://www.youtube.com/watch?v=-byQgOJ1kys" data-fancybox).b-play 
                           +img("ic-play2.svg")
      +rse()
   +cn-box()
   section.sec-rsult
      .rsult-bg 
         +img("rsult-ket-bg.png")
      .container 
         .rsult
            .rsult-wrap 
               .rsult-head.t-center.mb-40
                  +title("KẾT QUẢ","SAU KẾT QUẢ")
               .rsult-block.collapse-block
                  .rsult-panel.posi-rl
                     .rsult-md 
                        +img("ket-rsult-md.png")
                     .rsult-row.row 
                        .col.col-4
                           .rsult-it.collapse-item
                              .b-head.collapse-head
                                 h3.t-text TƯ DUY NGÔN NGỮ
                                 span.icon
                                    i.fa-light.fa-chevron-down
                              .b-body.collapse-body
                                 .mona-content 
                                    ul 
                                       li 
                                          | Giao tiếp tự tin với mẫu câu đơn giản
                                       li 
                                          | Nghe - hiểu và phản xạ tự nhiên
                        .col.col-4
                           .rsult-it.collapse-item
                              .b-head.collapse-head
                                 h3.t-text TỰ TIN THỂ HIỆN
                                 span.icon
                                    i.fa-light.fa-chevron-down
                              .b-body.collapse-body
                                 .mona-content 
                                    ul 
                                       li 
                                          | Khơi mở sáng tạo, biết đặt câu hỏi
                                       li   
                                          | Hình thành và phát triển kĩ năng cho trẻ
                        .col.col-4
                           .rsult-it.collapse-item
                              .b-head.collapse-head
                                 h3.t-text TƯ DUY PHẢN BIỆN
                                 span.icon
                                    i.fa-light.fa-chevron-down
                              .b-body.collapse-body
                                 .mona-content 
                                    ul 
                                       li 
                                          | Biết tập trung, theo nề nếp
                                       li 
                                          | Yêu thích học và khám phá
                        .col.col-4
                           .rsult-it.collapse-item
                              .b-head.collapse-head
                                 h3.t-text NỀN TẢNG TIẾNG ANH
                                 span.icon
                                    i.fa-light.fa-chevron-down
                              .b-body.collapse-body
                                 .mona-content 
                                    ul 
                                       li 
                                          | Biết tập trung, theo nề nếp
                                       li 
                                          | Yêu thích học và khám phá
   section.sec-int.ss-pd 
      .container 
         .int 
            .int-wrap 
               .int-head.t-center.mb-40
                  +title("CHỈ CÓ Ở YOLA","GIÁ TRỊ GIÁO DỤC & KIẾN THỨC VƯỢT NGOÀI ĐIỂM THI")
               .int-block 
                  .int-slider.slideSw 
                     .b-sw.posi-rl 
                        +SwiperControl("posi midle")
                        +Swiper()
                           .swiper-slide.col.col-6 
                              .int-it 
                                 a(href="").hidden-link 
                                 .b-inner 
                                    .b-img
                                       +img("int-img1.jpg")
                                    .b-ctn 
                                       h3.t-text 
                                          | KHỞI TRÍ TÒ MÒ
                                       .t-des 
                                          p 
                                             | Bắt đầu cùng trò chơi hỏi–đáp, bé luyện phản xạ nghe–nói với từ vựng và câu đơn giản
                                          p  Bé tự tin giới thiệu sở thích và vui học qua tương tác tự nhiên
                           .swiper-slide.col.col-6 
                              .int-it 
                                 .b-inner 
                                    .b-img
                                       +img("int-img2.jpg")
                                    .b-ctn 
                                       h3.t-text 
                                          | HỌC NGÔN NGỮ QUA ĐA MÔN & DỰ ÁN
                                       .t-des
                                          P 
                                             | Bé vừa chơi vừa học qua dự án đa lĩnh vực:
                                          P  
                                             | Tiếng Anh Cambridge, Lãnh đạo, Công nghệ và Thiết kế.
                                          P 
                                             | Hoạt động muôn màu giúp bé phát triển ngôn ngữ, tư duy, công nghệ và sáng tạo.
                           .swiper-slide.col.col-6 
                              .int-it 
                                 .b-inner 
                                    .b-img
                                       +img("int-img3.jpg")
                                    .b-ctn 
                                       h3.t-text 
                                          | CHIA SẺ & HOÀ NHẬP
                                       .t-des
                                          P
                                             | Bé chia sẻ ý tưởng, hợp tác với bạn và giữ lớp sạch sẽ. Hoạt động vui học này giúp bé nhanh hòa nhập, cùng bạn hợp tác cùng nhau vui chơi.
                  .mt-24 
                     +Btn("Kiểm tra trình độ miễn phí","","center")
   section.sec-taf.ss-pd-b 
      .container 
         .taf 
            .taf-wrap 
               .taf-head.t-center.mb-40
                  +title("LỊCH KHÓA HỌC","KHOÁ FLYERS")
               .taf-block 
                  .taf-table
                     table.table-custom
                        thead
                           tr 
                              th
                                 | CHI NHÁNH
                              th
                                 | KHAI GIẢNG
                              th
                                 | SỐ LƯỢNG HV
                              th
                                 | ĐỊA CHỈ
                              th 
                                 | BUỔI HỌC
                              th 
                                 | GIỜ HỌC
                        tbody 
                           each i,index in Array(7)
                              tr 
                                 td(data-text="CHI NHÁNH:") Đỗ Xuân Hợp
                                 td(data-text="KHAI GIẢNG:") 
                                    | 26/08/2025
                                 td(data-text="Số LƯỢNG HV:") 
                                    span.b-tag.b-live Còn chỗ
                                 td(data-text="ĐỊA CHỈ:")  
                                    span 299B Đỗ Xuân Hợp, Phường Phước Long B, TP. Thủ Đức, TP. Hồ Chí Minh
                                 td(data-text="BUỔI HỌC:") 
                                    span Thứ 3/5/7
                                 td(data-text="GIỜ HỌC:") 
                                    span 19:30 - 21:30
   +faq()
   section.sec-dkf.ss-pd
      .container 
         .dkf 
            .dkf-wrap 
               .dkf-row.row 
                  .col.col-6
                     .dkf-panel 
                        .t-head 
                           +title("","XÂY DỰNG LỊCH HỌC CHO BÉ")
                        .t-des 
                           p 
                              | Bạn không tìm thấy lịch phù hợp với bạn ?
                           p 
                              | Hãy chọn ngay cho chính bạn 1 lịch phù hợp
                  .col.col-6
                     .dkf-form 
                        form(action="")
                           .form-list.row 
                              .col.form-ip 
                                 input(type="text" placeholder="Tên của bạn")
                              .col.form-ip 
                                 input(type="text" placeholder="Số điện thoại")
                              .col.form-ip 
                                 .b-select 
                                    select(name="" data-placeholder="Chọn trình độ").re-select-main
                                       option(value="")
                                       option(value="") Demo
                                       option(value="") Demo
                              .col.form-ip 
                                 .b-select 
                                    select(name="" data-placeholder="Chọn địa điểm").re-select-main
                                       option(value="")
                                       option(value="") Demo
                                       option(value="") Demo
                              .col.form-ip 
                                 input(type="text" placeholder="Nhập ca học (VD: 08:00 - 07:00)")
                              .col.form-ip 
                                 input(type="text" placeholder="Nhập buổi học (VD: thứ 2-4-6")
                           .b-submit 
                              +Btn("Gửi yêu cầu","button","full")
   +card-sv()
   +form()